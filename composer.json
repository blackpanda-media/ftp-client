{
  "name": "speedy/ftp-client",
  "type": "library",
  "description": "A simple PHP FTP client",
  "keywords": [
    "ftp",
    "php"
  ],
  "homepage": "https://stagemedia.cz",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomáš Kliner",
      "email": "kliner.tomas@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.0",
    "ext-ftp": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.*"
  },
  "autoload": {
    "psr-4": {
      "Speedy\\FTP\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Speedy\\FTP\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}